quran[42]{c,v,t,f}:
  80,1,"وچولی یې تریو شو او مخ يې واړوه.",""
  80,2,"لدې امله چې ړوند ورته راغی.",""
  80,3,"او ته څه خبر يې ښايي هغه پاك شي.",""
  80,4,"يا پند واخلي او نصيحت څه ګټه ورته ورسوي.",""
  80,5,"خو څوك چې بې پروايي كوي.",""
  80,6,"نو ته ورپسې يې.",""
  80,7,"او نشته پر تا باندې ګناه که دی پاک نشې.",""
  80,8,"او هغه څوک چې راغلی دی تا ته منډي وهي.",""
  80,9,"او هغه وېريږي هم.",""
  80,10,"نو ته له هغه نه بې پروایې کوې.",""
  80,11,"هيڅكله نه، دا خو يو نصيحت دى.",""
  80,12,"نو د چا چې خوښه شي دا دې یاد کړي.",""
  80,13,"هغه په عزتمنو پاڼوكې (نغښتی) دی.",""
  80,14,"پورته کړی شوي دي، پاکې ساتلی شوي دي.",""
  80,15,"په لاسونو د لیکونکو ملایکو.",""
  80,16,"چې عزتمندې او نېکې دي.",""
  80,17,"تباه دې شي (دروغ ګڼونکی)انسان څومره غټ کافر دی.",""
  80,18,"له څه شي يې پيدا كړى دى؟",""
  80,19,"له يو څاڅكي (مني) څخه يې پيداکړ بيايې د هغه برخليك وټاكلو.",""
  80,20,"بيايې لار ورته آسانه كړه.",""
  80,21,"بيا يې هغه مړ كړ او قبر ته يې ورسولو.",""
  80,22,"بيا كله چې وغواړي نو را پورته به کړي ده لره.",""
  80,23,"هيڅكله نه، هغه لا هغه څه نه دي ترسره كړي چې الله ورته امر كړى.",""
  80,24,"نو انسان دې خپلو خوړو ته وګوري.",""
  80,25,"چې موږ ښې (پرېمانه) اوبه راتويې كړې دي.",""
  80,26,"بيا مو زمكه چولې (څيرلې) ده.",""
  80,27,"او په هغې كې مو (غلې) دانې را زرغونې كړې دي.",""
  80,28,"او انګور او سابه.",""
  80,29,"او زيتون او خرماوې.",""
  80,30,"او ګڼ ګڼ باغونه.",""
  80,31,"او مېوې او شينې ګیاوې.",""
  80,32,"ستاسي او ستاسي د څارويو د ګټې اخيستنې لپاره.",""
  80,33,"نو كله چې هغه كڼوونكې چيغه راشي.",""
  80,34,"هغه ورځ چې انسان به له خپله وروره تښتي.",""
  80,35,"او له خپلې مور او پلاره.",""
  80,36,"او له مېرمنې او زامنو يې.",""
  80,37,"د هغوى د هر سړي به هغه ورځ داسې حالت وي چې (له نورو يې) بې پروا كوي.",""
  80,38,"ځينې مخونه به هغه ورځ روښانه وي.",""
  80,39,"خندېدونكي خوشاله به وي.",""
  80,40,"او پرځينو مخونو به هغه ورځ دوړې وي.",""
  80,41,"توروالى به پرې خپور وي.",""
  80,42,"همدغه خلك كافران – فاجران دي.",""
