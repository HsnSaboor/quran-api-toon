quran[34]{c,v,t,f}:
  56,17,"Mmeranteε a wͻte nkwa mu daa na wͻnnwu da bɛdi wͻn hͻ akͻneaba (asom wͻn),",""
  56,18,"Wͻde nkoraa ne sanya nkuruwaa ne kyansee a nsa korͻgyee wͻ mu.",""
  56,19,"Wͻn ti mmpae wͻn, na wͻmmoro nso,",""
  56,20,"Ne nnuaba biara a wͻn ara bεhwε mu ayi deε wͻpε.",""
  56,21,"Ne nnomaa nnam biara a wͻn kͻn dͻ.",""
  56,22,"Ne (mmaa) akronkron ahoͻfεfoͻ a wͻn ani akεse-akεse,",""
  56,23,"(Wͻn ho fε) tesε nhwene pa bi a yɛahyε da de asie abͻ ho ban.",""
  56,24,"(Woi) yε akatua ma dwuma (pa) a wͻdiiε no.",""
  56,25,"Wͻnte nsεm hunu anaa kasa bͻne biara wͻ hͻ.",""
  56,26,"Gyesε ‘Salaaman Salaaman’ Asomdweε! Asomdweε!",""
  56,27,"Ɛna Nifasofoͻ no; hwan nom ne Nifaso foͻ no?",""
  56,28,"(Wͻn wͻ) nnua bi a εho yε trͻmtrͻm a nkasεε nni ho bi mu,",""
  56,29,"Ne nkwadu a abͻ esa-esa,",""
  56,30,"Ne nwunu a εbae trε mu,",""
  56,31,"Ne nsuo a εtene ba (abrε biara),",""
  56,32,"Ne nnuaba a abu soͻ,",""
  56,33,"(Nnuaba no berε) to ntwa, na yɛnsi wͻn ho kwan nso.",""
  56,34,"(Wͻn) wͻ aban soroͻ a εwͻ soro so.",""
  56,35,"Nokorε sε Yɛabͻ wͻn (mmeͻε foforͻ) paa,",""
  56,36,"Ɛna Yɛama wͻn ayε mmabunu,",""
  56,37,"Ɔdͻ ne ahomeka (wͻ wͻn mu); na wͻn (nyinaa) bͻ atipεn.",""
  56,38,"(Woi) wͻ Nifasofoͻ no.",""
  56,39,"Dodoͻ no bɛfiri tetefoͻ no mu,",""
  56,40,"Na dodoͻ no nso afiri nkyirimma yi mu.",""
  56,41,"Ɛna Benkum so foͻ no; hwan nom ne Benkum so foͻ no?",""
  56,42,"(Wͻn) wͻ mframa hyew ne nsuo hyeε mu,",""
  56,43,"Ne wisie tumm nwunu mu,",""
  56,44,"(Saa wisie no) nyε nwunu na (wͻnnya emu) anigyeε anaa papa biara.",""
  56,45,"Nokorε sε, kane no na wͻgye wͻn ani,",""
  56,46,"Na wͻtͻ kͻ bͻne akεseε so.",""
  56,47,"Na wͻka sε: “Sε yɛwu na yεyε mfuturo ne nnompe a, nokorε sε yεbεsan anyane yεn?",""
  56,48,"Anaasε yεn agyanom tetefoͻ no (nso)?",""
  56,49,"(Nkͻmhyεni), ka sε: “Tetefoͻ no ne nkyirimma no (nyinaa),",""
  56,50,"Nokorε sε yεbεboa wͻn ano akͻ εberε a yɛahyε no wͻ saa Da no mu no“.",""
