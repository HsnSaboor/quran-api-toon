quran[15]{c,v,t,f}:
  112,1,"Sakyk (o Muchammedai ﷺ ): „Jis yra Allahas, (Kuris yra) Vienas.",""
  112,2,"Allahas (As-Samad) Amžinas Prieglobstis (Allahas Sau-Pakankamas Viešpats, Kurio reikia visai kūrinijai, (nei Jis valgo nei geria)),",""
  112,3,"nei Jis gimdo, nei yra gimęs,",""
  112,4,"nei yra Jam ko nors lygaus.“",""
  113,1,"Sakyk: „Aš ieškau prieglobsčio pas aušros Viešpatį",""
  113,2,"nuo blogio, kurį Jis sukūrė,",""
  113,3,"ir nuo tamsos blogio, kai ji nusileidžia,",""
  113,4,"ir nuo pūtėjų į mazgus blogio (kurie praktikuoja magiją),",""
  113,5,"ir nuo blogio pavyduolio, kai jis pavydi.“",""
  114,1,"Sakyk: „Aš ieškau prieglobsčio pas žmonijos Viešpatį,",""
  114,2,"žmonijos Valdovą,",""
  114,3,"žmonijos Dievą,",""
  114,4,"nuo blogio pasišalinančio (nuo šnabždėjimo į žmogaus širdį, kai jis prisimena Allahą) šnabždūno (velnio, kuris šnabžda į žmogaus širdį),",""
  114,5,"kuris šnabžda (blogį) į žmonių krūtines,",""
  114,6,"iš džinų ir žmonių tarpo.“",""
